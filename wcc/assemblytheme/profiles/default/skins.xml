<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Sunburst Theme">
    <object name="wcc_assemblytheme"
        meta_type="Filesystem Directory View"
        directory="wcc.assemblytheme:skins/wcc_assemblytheme"/>
    <skin-path name="*">
        <layer name="wcc_assemblytheme"
                insert-after="custom"/>
    </skin-path>
</object>
